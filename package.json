{
  "name": "middleware",
  "version": "0.0.1",
  "scripts": {
    "fix": "sh ./eslint.sh",
    "start": "nodemon ./bin/www.js",
    "clean": "sh ./clean.sh",
    "prebuild": "npm run clean && babel bin --out-dir dist/bin &&cross-env BABEL_ENV=node babel client --out-dir dist/client --copy-files && babel server --out-dir dist/server --copy-files",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "postbuild": "babel public --out-dir dist/public --copy-files && babel test --out-dir dist/test --copy-files"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "hbs": "^4.0.1",
    "history": "^4.7.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-manifest-plugin": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "html-webpack-plugin": "^3.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2"
  }
}
