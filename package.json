{
  "name": "middleware",
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./bin/www.js --exec babel-node",
    "prebuild": "babel bin --out-dir dist/bin && babel client --out-dir dist/client --copy-files && babel server --out-dir dist/server --copy-files",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "postbuild": "babel public --out-dir dist/public --copy-files"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "html-webpack-plugin": "^3.1.0",
    "less-loader": "^4.1.0",
    "node-jsx": "^0.13.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.2"
  }
}
